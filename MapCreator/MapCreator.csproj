<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net7.0-windows</TargetFramework>
		<UseWindowsForms>true</UseWindowsForms>
		<Nullable>disable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<StartupObject>MapCreator.Program</StartupObject>
		<ApplicationIcon>res\exeicon\mC_0001ico.ico</ApplicationIcon>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
		<OutputPath>..\Output</OutputPath>
	</PropertyGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Utilities\Utilities.csproj" />
	</ItemGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.WindowsDesktop.App" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Update="Interface\BuildLogger.cs">
	    <SubType>Form</SubType>
	  </Compile>
	  <Compile Update="Interface\CanvasControlBox.cs">
	    <SubType>Form</SubType>
	  </Compile>
	  <Compile Update="Interface\StaticSelector.cs">
	    <SubType>Form</SubType>
	  </Compile>
	</ItemGroup>

</Project>